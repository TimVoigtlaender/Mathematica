Cell[BoxData[
 RowBox[{
  RowBox[{"MyPrimes", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"L", "=", 
       RowBox[{"Table", "[", 
        RowBox[{"t", ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ",", 
      RowBox[{"i", "=", "2"}], ",", "k"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"L", "=", 
      RowBox[{"ReplacePart", "[", 
       RowBox[{"L", ",", "0", ",", "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"i", "*", "i"}], "\[LessEqual]", "n"}], ",", 
       RowBox[{
        RowBox[{"k", "=", 
         RowBox[{"i", "+", "i"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"While", "[", 
         RowBox[{
          RowBox[{"k", "\[LessEqual]", "n"}], ",", 
          RowBox[{
           RowBox[{"L", "=", 
            RowBox[{"ReplacePart", "[", 
             RowBox[{"L", ",", "0", ",", "k"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"k", "=", 
            RowBox[{"k", "+", "i"}]}]}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"i", "=", 
         RowBox[{"i", "+", "1"}]}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"Select", "[", 
       RowBox[{"L", ",", "Positive"}], "]"}], "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{
