     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"t", ",", "r"}], "}"}], "=", 
       RowBox[{"Timing", "@", 
        RowBox[{"f", "[", "bound", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<running time: \>\"", ",", "t"}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{"\"\<# of found primes: \>\"", ",", 
