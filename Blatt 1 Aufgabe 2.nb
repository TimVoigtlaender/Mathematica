Cell[BoxData[
 RowBox[{
  RowBox[{"MathPrimes", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"L", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"NextPrime", "[", "t", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "1", ",", 
           RowBox[{"n", "+", "1"}]}], "}"}]}], "]"}]}], ",", "i"}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "n"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"L", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "\[Equal]", 
          RowBox[{"L", "[", 
           RowBox[{"[", 
            RowBox[{"i", "+", "1"}], "]"}], "]"}]}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"L", "=", 
          RowBox[{"ReplacePart", "[", 
           RowBox[{"L", ",", 
            RowBox[{"n", "+", "1"}], ",", "i"}], "]"}]}]}], "]"}]}], "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"Select", "[", 
       RowBox[{"L", ",", 
        RowBox[{
         RowBox[{"#", "\[LessEqual]", "n"}], "&"}]}], " ", "]"}], "]"}]}]}], 
   "]"}]}]], "Input",
